@model SAEC.PROJETOTESTE.MVC.ViewModels.CidadeViewModel

@{
    ViewBag.Title = "Create";
}

<div class="barra-topo-header">
    <h2>Cidades</h2>
</div>

<div class="conteudo-central">
    <div class="conteudo-central-2">
        <div class="conteudo-central-2-titulo">
            <h5>Cadastro de Cidade</h5>
        </div>
        <div class="conteudo-central-2-conteudo">
            @using (Html.BeginForm()) 
            {
                @Html.Partial("CreateOrEdit", Model)
                
                <input type="submit" value="Salvar" />
                <input type="button" onclick="location.href='@Url.Content("~/Cidade/Index")'" value="Voltar" />
            }
        </div>
    </div>
</div>